{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Bridge Debug",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/react/**",
        "node_modules/react-dom/**",
        "node_modules/scheduler/**",
      ]
    }
  ]
}
