{
  "extends": "@synthetixio/synpress-tsconfig/base.json",
  "compilerOptions": {
    "baseUrl": "../../../../",
    "types": ["cypress", "./cypress.d.ts", "@testing-library/cypress"],
    "outDir": "./output",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "exactOptionalPropertyTypes": false
  },
  "include": [
    "../../../../cypress.config.ts",
    "../../../../cypress.cctp.config.ts",
    "../support/*.*",
    "**/*.*"
  ],
  "files": ["environment.d.ts"]
}
